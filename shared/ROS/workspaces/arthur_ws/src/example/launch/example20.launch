<?xml version="1.0"?>

<launch>


<param name="Rb" type="double" value="2.0"/>
<param name="d" type="double" value="10.0"/>
<param name="c" type="double" value="36.0"/>
<param name="ref_vel" type="double" value="1.0"/>


<!--Run the stage simulator-->
<node pkg = "stage_ros" name = "stageros" type = "stageros" output = "screen" args="-d $(find example)/worlds/map_5-2-3-1-7-2.world">
</node>

<!-- Communication manager args="N_robots"-->
<node pkg = "example" name = "communication" type = "communication.py" args="0 0 0 0 0 1 1 2 2 2 3 4 4 4 4 4 4 4 5 5" output="screen">
</node>

<!--Run the cntroller node args="robot_number robot_group" -->
<group ns = "robot_0">
    <node pkg = "example" name = "controller" type = "dubins-seg.py" args="0 0 20.000 0.000 0.000 90.000" output="screen">
    </node>
</group>
<group ns = "robot_1">
    <node pkg = "example" name = "controller_1" type = "dubins-seg.py" args="1 0 28.284 28.284 0.000 135.000" output="screen">
    </node>
</group>
<group ns = "robot_2">
    <node pkg = "example" name = "controller_2" type = "dubins-seg.py" args="2 0 -30.000 0.000 0.000 90.000" output="screen">
    </node>
</group>
<group ns = "robot_3">
    <node pkg = "example" name = "controller_3" type = "dubins-seg.py" args="3 0 1.000 10.000 0.000 0.000" output="screen">
    </node>
</group>
<group ns = "robot_4">
    <node pkg = "example" name = "controller_4" type = "dubins-seg.py" args="4 0 0.000 -30.000 0.000 180.000" output="screen">
    </node>
</group>
<group ns = "robot_5">
    <node pkg = "example" name = "controller_5" type = "dubins-seg.py" args="5 1 10.000 0.071 0.000 -90.000" output="screen">
    </node>
</group>
<group ns = "robot_6">
    <node pkg = "example" name = "controller_6" type = "dubins-seg.py" args="6 1 30.000 0.000 0.000 -90.000" output="screen">
    </node>
</group>
<group ns = "robot_7">
    <node pkg = "example" name = "controller_7" type = "dubins-seg.py" args="7 2 -3.473 19.696 0.000 190.000" output="screen">
    </node>
</group>
<group ns = "robot_8">
    <node pkg = "example" name = "controller_8" type = "dubins-seg.py" args="8 2 -50 0.001 0.000 90.000" output="screen">
    </node>
</group>
<group ns = "robot_9">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="9 2 0.000 -20.000 0.000 0.000" output="screen">
    </node>
</group>
<group ns = "robot_10">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="10 3 -40 0.000 0.000 -90.000" output="screen">
    </node>
</group>
<group ns = "robot_11">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="11 4 0.000 30.000 0.000 0.000" output="screen">
    </node>
</group>
<group ns = "robot_12">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="12 4 -10.000 0.000 0.000 90.000" output="screen">
    </node>
</group>
<group ns = "robot_13">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="13 4 0.000 40.000 0.000 180.000" output="screen">
    </node>
</group>
<group ns = "robot_14">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="14 4 0.000 50.000 0.000 0.000" output="screen">
    </node>
</group>
<group ns = "robot_15">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="15 4 0.000 -50.000 0.000 180.000" output="screen">
    </node>
</group>
<group ns = "robot_16">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="16 4 50.000 0.000 0.000 -90.000" output="screen">
    </node>
</group>
<group ns = "robot_17">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="17 4 60.000 0.000 0.000 90.000" output="screen">
    </node>
</group>
<group ns = "robot_18">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="18 5 40.000 0.000 0.000 90.000" output="screen">
    </node>
</group>
<group ns = "robot_19">
    <node pkg = "example" name = "controller_9" type = "dubins-seg.py" args="19 5 -40.000 30.000 0.000 90.000" output="screen">
    </node>
</group>



<!--Run the rviz for better visualization-->
